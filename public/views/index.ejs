<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <title>OCM || Home</title>
  <meta name="robots" content="noindex, follow" />
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta http-equiv="Cache-control" content="public">
  <meta name="theme-style-mode" content="0"> <!-- 0 == light, 1 == dark -->


  <!-- Favicon -->
  <link rel="shortcut icon" type="image/x-icon" href="assets/images/favicon.png">
  <!-- CSS 
    ============================================ -->
  <link rel="stylesheet" href="assets/css/vendor/bootstrap.min.css">
  <link rel="stylesheet" href="assets/css/vendor/slick.css">
  <link rel="stylesheet" href="assets/css/vendor/slick-theme.css">
  <link rel="stylesheet" href="assets/css/vendor/nice-select.css">
  <link rel="stylesheet" href="assets/css/plugins/feature.css">
  <link rel="stylesheet" href="assets/css/plugins/jquery-ui.min.css">

  <!-- Style css -->
  <link rel="stylesheet" href="assets/css/style.css">
</head>


<body class="template-color-1">

  <!-- start header area -->
  <!-- Preloader -->
  <div class="preloader">
    <div class='loader'>
      <div class='circle'></div>
      <div class='circle'></div>
      <div class='circle'></div>
      <div class='circle'></div>
      <div class='circle'></div>
    </div>
  </div>

  <!-- popup alert for beta testers -->
  <div class="devnet-alert">
    <span class="devnet-pop-helper"></span>
    <div>
      <div class="devnet-pop-close">&times;</div>
      <p style="color: red;">This website is currently in <strong>Beta!</strong> <br> This means any transactions made on this website <strong>must be done through the XLS-20 devnet</strong>. Connect your XUMM wallet to the devnet then proceed with using this website. <br> Should you fail to do so and you make a transaction, those<strong> real world funds will be transfered and lost</strong>.</p>
    </div>
  </div>


  <!--Require header-->
  <%- include( 'sections/header.ejs'); -%>
  <div class="halloween">
    <img src="assets/images/halloween/bat.png" class="bat" alt="">
    <img src="assets/images/halloween/ghost.png" class="ghost" alt="">
    <img src="assets/images/halloween/spiderweb.png" class="web" alt="">
    <img src="assets/images/halloween/torch.png" class="torch" alt="">
  </div>

  <!-- start banner area -->
  <div class="slider-one rn-section-gapTop">
    <div class="container">
      <div class="row row-reverce-sm align-items-center">
        <div class="col-lg-5 col-md-6 col-sm-12 mt_sm--50">
          <h2 class="title" data-sal-delay="200" data-sal="slide-up" data-sal-duration="800">Welcome To The OnChain Marketplace</h2>
          <p class="slide-disc" data-sal-delay="300" data-sal="slide-up" data-sal-duration="800">Keeping custody where it belongs. <br>Setting a new standard with feeless trading.</p>
          <div class="button-group">
            <a class="btn btn-large btn-primary" href="explore?page=0" data-sal-delay="400" data-sal="slide-up" data-sal-duration="800">Explore Now!</a>
          </div>
        </div>
        <div class="col-lg-5 col-md-6 col-sm-12 offset-lg-1">
          <div class="slider-thumbnail float">
            <img src="assets/images/slider/pumpkin.png" alt="Slider Images" draggable="false">
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- End banner area -->

  <!-- start service area -->
  <div class="rn-service-area rn-section-gapTop">
    <div class="container">
      <div class="row">
        <div class="col-12 mb--50">
          <h3 class="title" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">Create and sell your NFTs on the XRPL</h3>
        </div>
      </div>
      <div class="row g-5">
        <!-- start single service -->
        <div class="col-xxl-3 col-lg-4 col-md-6 col-sm-6 col-12">
          <div data-sal="slide-up" data-sal-delay="150" data-sal-duration="800" class="rn-service-one color-shape-7">
            <div class="inner">
              <div class="frontpage-icon">
                <img src="assets/images/icons/shape-7.png" alt="Shape">
              </div>
              <div class="subtitle">Step-01</div>
              <div class="content">
                <h4 class="title"><a href="connect">Connect your wallet</a></h4>
                <p class="description">Sign-In to our platform with your XUMM Account. </p>
                <a class="read-more-button" href="connect"><i class="feather-arrow-right"></i></a>
              </div>
            </div>
            <a class="over-link" href="connect"></a>
          </div>
        </div>
        <!-- End single service -->
        <!-- start single service -->
        <div class="col-xxl-3 col-lg-4 col-md-6 col-sm-6 col-12">
          <div data-sal="slide-up" data-sal-delay="250" data-sal-duration="800" class="rn-service-one color-shape-5">
            <div class="inner">
              <div class="frontpage-icon">
                <img src="assets/images/icons/shape-5.png" alt="Shape">
              </div>
              <div class="subtitle">Step-02</div>
              <div class="content">
                <h4 class="title"><a href="redeem">Redeem your NFT's</a></h4>
                <p class="description">Use our Redeem Page to trade your IOU token for an NFT.</p>
                <a class="read-more-button" href="redeem"><i class="feather-arrow-right"></i></a>
              </div>
            </div>
            <a class="over-link" href="redeem"></a>
          </div>
        </div>
        <!-- End single service -->
        <!-- start single service -->
        <div class="col-xxl-3 col-lg-4 col-md-6 col-sm-6 col-12">
          <div data-sal="slide-up" data-sal-delay="300" data-sal-duration="800" class="rn-service-one color-shape-6">
            <div class="inner">
              <div class="frontpage-icon">
                <img src="assets/images/icons/shape-6.png" alt="Shape">
              </div>
              <div class="subtitle">Step-03</div>
              <div class="content">
                <h4 class="title"><a href="explore">Explore the Marketplace</a></h4>
                <p class="description">Explore, Buy, Sell, Mint and Burn</p>
                <a class="read-more-button" href="explore"><i class="feather-arrow-right"></i></a>
                <p class="finePrint" style="color: red;">Platform in Beta</p>
              </div>
            </div>
            <a class="over-link" href="explore?page=0"></a>
          </div>
        </div>
        <!-- End single service -->
      </div>
    </div>
  </div>
  <div class="rn-live-bidding-area rn-section-gapTop">
    <div class="container">
        <div class="row mb--50">
            <div class="col-lg-12">
                <div class="section-title">
                    <h3 class="title mb--0 live-bidding-title" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">Most Viewed</h3>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="banner-one-slick slick-activation-03 slick-arrow-style-one rn-slick-dot-style slick-gutter-15">
                  <%mostViewedNFTs.forEach(function(nft) {%>
                    <div class="single-slide-product">
                      <%- include ('models/nft-card-index.ejs', {nft:nft})%>
                    </div>
                  <%});%>
                </div>
            </div>
        </div>
    </div>
</div>
  <%- include('sections/popup-modal.ejs') -%>
  <%- include('sections/footer.ejs') -%>

  <!-- Start Top To Bottom Area  -->
  <div class="rn-progress-parent">
    <svg class="rn-back-circle svg-inner" width="100%" height="100%" viewBox="-1 -1 102 102">
      <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
    </svg>
  </div>
  <!-- End Top To Bottom Area  -->
  <!-- JS ============================================ -->
  <script src="assets/js/vendor/jquery.js"></script>
  <script src="assets/js/vendor/jquery.nice-select.min.js"></script>
  <script src="assets/js/vendor/jquery-ui.js"></script>
  <script src="assets/js/vendor/modernizer.min.js"></script>
  <script src="assets/js/vendor/feather.min.js"></script>
  <script src="assets/js/vendor/slick.min.js"></script>
  <script src="assets/js/vendor/bootstrap.min.js"></script>
  <script src="assets/js/vendor/sal.min.js"></script>
  <script src="assets/js/vendor/waypoint.js"></script>
  <script src="assets/js/vendor/wow.js"></script>
  <script src="assets/js/vendor/particles.js"></script>
  <script src="assets/js/vendor/jquery.style.swicher.js"></script>
  <script src="assets/js/vendor/js.cookie.js"></script>
  <script src="assets/js/vendor/count-down.js"></script>
  <script src="assets/js/vendor/counter-up.js"></script>
  <script src="assets/js/vendor/isotop.js"></script>
  <script src="assets/js/vendor/imageloaded.js"></script>
  <script src="assets/js/vendor/backtoTop.js"></script>
  <script src="assets/js/vendor/scrolltrigger.js"></script>
  <script src="assets/js/vendor/jquery.custom-file-input.js"></script>
  <script src="assets/js/vendor/savePopup.js"></script>
  <!-- main JS -->
  <script src="assets/js/main.js"></script>
  <script>
    $(window).load(function () {
    $('.devnet-alert').click(function(){
        $('.devnet-alert').hide();
    });
    $('.devnet-pop-close').click(function(){
        $('.devnet-alert').hide();
    });
});
  </script>
</body>

</html>